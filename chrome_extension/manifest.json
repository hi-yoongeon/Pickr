{
    "name" : "Pickr",
    "descriptions" : "소셜이미지 북마킹 서비스",
    "version" : "0.0.2",
    "background_page" : "background.html",
    "icons" : {
    	"19" : "pickr_chrome_icon.png",
	"48" : "pickr_icon_48.png",
	"128" : "pickr_icon_128.png"
    },
    "permissions" : [
	"http://*/",
	"tabs"
    ],
    "content_scripts" : [
	{
	    "matches" : ["http://*/*", "https://*/*"],
	    "js" : [
		"jigu-latest.min.js",
		"contentscript.js"
	    ],
	    "run_at" : "document_end"
	}
    ],
    "browser_action": {
	"default_icon" : "pickr_icon_19.png",
	"default_title" : "Pickr"
    },
    "options_page" : "pickr_option.html"
}
